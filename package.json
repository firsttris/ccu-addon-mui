    "react-router-dom": "^6.20.1",
    "react-use-websocket": "^4.11.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.11.0",
    "@iconify/react": "^5.2.0",
    "@nx/eslint": "20.3.1",
