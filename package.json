    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "1.3.1",
    "@vitest/ui": "1.3.1",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
